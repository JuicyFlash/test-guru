<table>
  <tr>
    <th>Вопросы</th>

  </tr>
  <% @test.questions.each do |question| %>
    <tr>
      <td><%= question.body %></td>
      <td><%= link_to 'Изменить', edit_question_path(question) %></td>
      <td><%= link_to 'Удалить', question_path(question), method: :delete %></td>
    </tr>
  <% end %>
</table>
<br/>

<%= link_to 'Создать новый вопрос', new_test_question_path(@test) %>
